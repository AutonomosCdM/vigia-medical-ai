{
  "ServiceName": "vigia-medical-ai-agent-smith",
  "SourceConfiguration": {
    "ImageRepository": {
      "ImageIdentifier": "586794472237.dkr.ecr.us-east-1.amazonaws.com/vigia-medical-ai:latest",
      "ImageConfiguration": {
        "Port": "8000",
        "RuntimeEnvironmentVariables": {
          "AWS_DEPLOYMENT": "true",
          "PORT": "8000",
          "VIGIA_ENV": "production"
        }
      },
      "ImageRepositoryType": "ECR"
    },
    "AutoDeploymentsEnabled": false
