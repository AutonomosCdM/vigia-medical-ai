  "EncryptionConfiguration": {
    "KmsKey": "alias/aws/apprunner"
  },
  "InstanceConfiguration": {
    "Cpu": "1 vCPU",
    "Memory": "2 GB"
  },
  "Tags": [
    {
      "Key": "Project",
      "Value": "VIGIA Medical AI"
    },
    {
      "Key": "Environment",
      "Value": "Production"
    },
    {
      "Key": "Landing",
      "Value": "Agent Smith"
    }
  ]
}